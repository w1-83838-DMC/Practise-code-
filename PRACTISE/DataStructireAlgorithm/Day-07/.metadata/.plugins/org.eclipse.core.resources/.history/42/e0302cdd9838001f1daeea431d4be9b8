package com.sunbeam;

import java.util.Scanner;

public class BStreeMain {
	public static void main(String[] args) {
		BStree bStree= new BStree();
		Scanner sc= new Scanner(System.in);
		int choice=0;
		do {
			System.out.println("0.Exit 1.Add 2. Delete ");
			choice=sc.nextInt();
			switch(choice)
			{
				case 1: 
					bStree.addNode(12);
					bStree.addNode(45);
					bStree.addNode(1000);
					break;
				case 2: 
					bStree.preorder();
					break;
			}
			
		}while(choice!=0);
	}

}
	

