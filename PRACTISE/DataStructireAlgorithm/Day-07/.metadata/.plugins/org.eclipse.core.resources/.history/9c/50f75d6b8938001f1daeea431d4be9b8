package com.sunbeam;

public class List3 {
	static class Node{
		private int data;
		private Node next;
		private Node prev;
		public Node(int value)
		{
			data=value;
			next=prev=null;
		}
	}
	
	private Node head;
	public List3()
	{
		head=null;
	}
	public boolean isEmpty(){
		return head==null;
	}
	public void addFirst(int value)
	{
		// create Node 
		Node newnode=new Node(value);
		// if empty
		if(head==null)
		{
			// addnewnode into head 
			head=newnode;
			// make list circular 
			
		}
		
		
	}
	
	

}
