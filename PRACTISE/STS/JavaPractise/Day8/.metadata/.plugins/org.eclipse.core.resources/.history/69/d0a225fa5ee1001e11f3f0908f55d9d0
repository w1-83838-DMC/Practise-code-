package p1;

public class Manager extends Employee{
	private int managerid;
	private int salary;
	
	public Manager()
	{
		super();
		
	}
	public Manager(String name,int age,int empid, String department,int managerid,int salary)
	{
		super(name,age,empid,department);
		this.managerid=managerid;
		this.salary=salary;
	}
	public void display()
	{
		System.out.println("Manager id "+ managerid + "Salary "+ salary);
		
	}

}
