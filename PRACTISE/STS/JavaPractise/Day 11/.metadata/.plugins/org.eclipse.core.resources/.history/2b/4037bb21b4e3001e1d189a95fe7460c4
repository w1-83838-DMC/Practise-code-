class Test<T>
{
	void display(T value)
	{
		System.out.println("Value ="+value);
	}
}
public class Day11_10 {
	public static void main(String[] args) {
		Test<Integer> t1= new Test<Integer>();
		
		Test<Float> t2=new Test<Float>();
		
		Test<Double> t3= new Test<Double>();

		t1.display(10);
		t2.display(12.8f);
		t3.display(13.00);

	}

}
