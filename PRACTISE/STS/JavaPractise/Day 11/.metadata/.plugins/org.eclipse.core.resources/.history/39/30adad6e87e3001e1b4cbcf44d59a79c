class Demo<T>
{
	T num; // filed of a class
	Demo() // parameterless Constructor
	{
		
	}
	Demo(T num) // parameterized Constructor is taking one argument of type T which is generic type 
	{
		this.num=num;
	}
	Demo(T n1,T n2)
	{
		
	}
	void display()
	{
		System.out.println(" Value "+ this.num);
		
	}
}
public class Day11_6 {

	public static void main(String[] args) {
		Demo d1= new Demo(12); // raw type
		Demo d2= new Demo(12,67);
		d1.display();
	}
}
