class Demo<T>
{
	T num; // filed of a class
	Demo() // parameterless Constructor
	{
		
	}
	Demo(T num) // parameterized Constructor
	{
		this.num=num;
	}
	void display()
	{
		System.out.println(" Value "+ this.num);
		
	}
}
public class Day11_6 {

	public static void main(String[] args) {
		Demo d1= new Demo();
		d1.display();
	}
}
