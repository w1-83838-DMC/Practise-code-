// HashMap
// LinkedHashMap
// TreeMap

// HashMap
// unordered and unsorted
// 

import java.util.HashMap;
public class Day13_3
{
	public static void main(String[] args) {
		HashMap<Integer,String> h1= new HashMap<Integer,String>();
		h1.put(10,"PG-DMC");
		h1.put(20, "PG-DAC");
		h1.put(30, "PG-DESD");
		h1.put(40, "PG-DITISS");
		h1.put(40, "Pg-dac");
		
		System.out.println("Hash map "+ h1);
	}
}


