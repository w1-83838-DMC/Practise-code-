// deleting the file
import java.io.IOException;
public class Day13_7
{
	public static void main(String[] args) {
		try {
		String fname="Demo.dat";
		File f1=new File(fname);
		boolean b1=f1.delete();
		System.out.println("Status " + b1);
		}catch(Exception e)
		{
			e.printStackTrace();
			
		}
		
	}
}
