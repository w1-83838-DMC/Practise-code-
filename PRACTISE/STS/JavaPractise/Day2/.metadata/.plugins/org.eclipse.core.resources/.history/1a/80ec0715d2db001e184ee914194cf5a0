/* Non primitive to primitive */
class Day2_4
{
	public static void main(String[] args) 
	{
		
		String s1="12345abc";
		int result=Integer.parseInt(s1);
		System.out.println(s1);
		System.out.println("Result = " + result);  // getting NumberFormatException
		
		
	}
}
