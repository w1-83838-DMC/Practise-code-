/*toString() is not overridden  inside The Employee class */
class employee
{
	private String name;
	private String dept;
	private String designation;
	private int empid;
	private int salary;
	
	employee()
	{
		this.name="Ajay";
		this.dept="DMC";
		this.designation="Software Developer";
		this.empid=4567;
		this.salary=90000;	
	}
	void dispData()
	{
		System.out.println("Name = "+this.name+" EMPID = "+this.empid);
		System.out.println("Salary = "+this.salary);
		System.out.println("Dept = "+this.dept+" Desg = "+this.desg);
	}
}

public class Day4_1 {
	public static void main(String[] args) {
		employee e1= new employee();
		
	}

}
