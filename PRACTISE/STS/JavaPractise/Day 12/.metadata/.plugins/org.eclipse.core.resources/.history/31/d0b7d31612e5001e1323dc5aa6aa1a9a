import java.util.ArrayList;

class Employee
{
	String name;
	int empid;
	public Employee()
	{
		
	}
	public Employee(String name,int empid)
	{
		this.empid= empid;
		
	}
	
}

public class Day12_4 {
	public static void main(String[] args)
	{
		ArrayList<Employee>a1= new ArrayList<Employee>();
		Employee e1= new Employee("emp1",123);
		
	}

}
