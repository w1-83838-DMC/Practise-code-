class Date
{
	int dd;
	int mm;
	int yy;
	
	Date()
	{
		
	}
	Date(int dd, int mm, int yy)
	{
		this.dd=dd;
		this.mm=mm;
		this.yy=yy;
	}
	void display()
	{
		System.out.println(this.dd +"/"+ this.mm +"/"+ this.yy);
	}
	
}
class student
{
	int rollno;  // filed
	int marks;   // filed
	Date dobj;   // Reference
	
	
	student()
	{
		dobj=new Date();
		
		
	}
	student(int rollno, int marks)
	{
		this.rollno=rollno;
		this.marks=marks;
	}
	void printData()
	{
		System.out.println(" Roll no = "+ this.rollno + " Marks ="+ this.marks );
		dobj.display();
		
	}
}
