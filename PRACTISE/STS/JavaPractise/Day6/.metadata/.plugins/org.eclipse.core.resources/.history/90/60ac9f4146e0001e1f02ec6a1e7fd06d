class Day6_6
{
	
	static void swap(int arr[])
	{
		int temp=arr[0];
		arr[0]=arr[1];
		arr[1]= temp;
		
	}
	public static void main(String[] args) {
		int num1=10;
		int num2=20;
		int arr[]= {num1,num2};
		System.out.println("num1 and num2 "+ num1 + "\t" + num2);
		swap(arr);
		System.out.println("num1 and num2 "+ num1 + "\t" + num2);
		
	}
}